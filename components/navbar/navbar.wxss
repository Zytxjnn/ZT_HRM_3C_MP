@font-face {
    font-family: "iconfont"; /* Project id 2939263 */
    src: url('//at.alicdn.com/t/font_2939263_682yz9zyh9i.woff2?t=1638673079266') format('woff2'),
         url('//at.alicdn.com/t/font_2939263_682yz9zyh9i.woff?t=1638673079266') format('woff'),
         url('//at.alicdn.com/t/font_2939263_682yz9zyh9i.ttf?t=1638673079266') format('truetype');
  }
  
  .iconfont {
    font-family: "iconfont" !important;
    font-size: 16px;
    font-style: normal;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
  }
  
  .icon-back:before {
    content: "\e697";
  }

.nav {
  position: fixed;
  left: 0;
  top: 0;
  width: 100vw;
  padding:0 20rpx ;
  box-sizing: border-box;
  margin-bottom: 20rpx;
  background-color: #fff;
}
.nav-main {
  width: 100%;
  height: 100%;
  position: relative;
}
.nav .capsule-box {
  position: absolute;
  box-sizing: border-box;
  width: 100%;
}

.title{
    position: relative;
    text-align: center;
}


.icon-back{
    position: absolute;
    left: 0;
    top: 50%;
    transform: translateY(-50%);
    font-weight: 800;
    font-size: 40rpx;
}